{
  "name": "polis",
  "version": "0.0.0",
  "description": "polis =====",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch & nodemon --inspect=0.0.0.0:9229 dist/index.js",
    "debug": "SERVER_LOG_LEVEL=debug nodemon --inspect=0.0.0.0:9229 dist/index.js",
    "serve": "node --max_old_space_size=2048 --gc_interval=100 dist/index.js",
    "start": "npm run build && npm run serve",
    "dev": "npm install && npm run build:watch",
    "type:check:watch": "tsc --watch",
    "format": "prettier --config ./prettier.config.js --no-editorconfig '**/*.ts' --write",
    "format:check": "prettier --config ./prettier.config.js --no-editorconfig '**/*.ts' --check",
    "lint": "eslint --quiet .",
    "lint:verbose": "eslint .",
    "db:reset": "node bin/db-reset.js",
    "db:schema": "pg_dump \"${DATABASE_URL}\" --schema-only --no-owner --no-privileges --file=schema.sql",
    "test": "if [ -z \"$NODE_EXTRA_CA_CERTS\" ] && command -v mkcert >/dev/null 2>&1; then export NODE_EXTRA_CA_CERTS=$(mkcert -CAROOT)/rootCA.pem; fi && jest",
    "test:unit": "npm test -- /__tests__/unit/",
    "test:integration": "npm test -- /__tests__/integration/",
    "test:feature": "npm test -- /__tests__/feature/ --testPathIgnorePatterns=[]"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/pol-is/polisServer.git"
  },
  "author": "",
  "license": "AGPL-3.0",
  "engine": ">=22.11.0",
